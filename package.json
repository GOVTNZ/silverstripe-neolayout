{
  "name": "neolayout",
  "private": true,
  "version": "0.0.0",
  "dependencies": {
    "jquery": "2.1.1",
    "node-uuid": "1.4.1",
    "react": "0.12.1"
  },
  "devDependencies": {
    "browserify": "7.0.0",
    "del": "0.1.3",
    "gulp": "3.8.10",
    "gulp-react": "2.0.0",
    "gulp-stylus": "1.3.4",
    "gulp-uglify": "1.0.1",
    "jest-cli": "0.2.0",
    "jquery-simulate-ext": "1.3.0",
    "jsdoc-to-markdown": "0.5.9",
    "react-tools": "0.12.1",
    "vinyl-transform": "1.0.0"
  },
  "scripts": {
    "build": "gulp",
    "build:debug": "gulp js:debug",
    "docs": "gulp js:debug && jsdoc2md ./javascript/build/**/*.js > docs/client_app_reference.md && gulp clean:js",
    "postinstall": "npm run build",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "scriptPreprocessor": "./tests/jest/preprocessor.js",
    "testDirectoryName": "tests/jest/tests",
    "unmockedModulePathPatterns": [
      "./node_modules/react",
      "./node_modules/node-uuid",
      "./tests/jest/fixtures"
    ]
  }
}
